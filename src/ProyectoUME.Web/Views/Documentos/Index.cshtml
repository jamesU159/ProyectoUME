@model IEnumerable<ProyectoUME.Core.Domain.Documentos>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CursoAltura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CertificadoEps)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CertificadoArl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CertificadoPensiones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HojaVida)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CursoAltura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CertificadoEps)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CertificadoArl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CertificadoPensiones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HojaVida)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTramiteD">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTramiteD">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTramiteD">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
