@model IEnumerable<ProyectoUME.Core.Domain.TurnoLaboral>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdObrero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoProyecto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jornada)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdObrero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoProyecto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jornada)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NumeroConsulta">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NumeroConsulta">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NumeroConsulta">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
